{
  "description": "Run DuckHog tests with Flight SQL server",
  "on_init": "SET logging_level = 'INFO';",
  "autoloading": "all",
  "statically_loaded_extensions": [
    "duckhog"
  ],
  "test_env": [
    {
      "env_name": "FLIGHT_HOST",
      "env_value": "127.0.0.1"
    },
    {
      "env_name": "FLIGHT_PORT",
      "env_value": "8815"
    },
    {
      "env_name": "DUCKHOG_USER",
      "env_value": "postgres"
    },
    {
      "env_name": "DUCKHOG_PASSWORD",
      "env_value": "postgres"
    }
  ]
}
