# name: test/sql/roadmap/rm16_on_conflict_do_nothing_remote.test_slow
# description: Roadmap target RM16 - INSERT ... ON CONFLICT DO NOTHING on remote table with unique constraints
# group: [roadmap]

require duckhog

require-env FLIGHT_HOST

require-env FLIGHT_PORT

require-env DUCKHOG_USER

require-env DUCKHOG_PASSWORD

statement ok
ATTACH 'hog:memory?user=${DUCKHOG_USER}&password=${DUCKHOG_PASSWORD}&flight_server=grpc+tls://${FLIGHT_HOST}:${FLIGHT_PORT}&tls_skip_verify=true' AS remote_flight;

statement ok
DROP SCHEMA IF EXISTS remote_flight.roadmap_rm16 CASCADE;

statement ok
CREATE SCHEMA remote_flight.roadmap_rm16;

statement ok
CREATE TABLE remote_flight.roadmap_rm16.t(i INT PRIMARY KEY, v INT);

statement ok
INSERT INTO remote_flight.roadmap_rm16.t VALUES (1, 10);

statement ok
INSERT INTO remote_flight.roadmap_rm16.t VALUES (1, 20) ON CONFLICT(i) DO NOTHING;

query I
SELECT COUNT(*) FROM remote_flight.roadmap_rm16.t;
----
1

query I
SELECT v FROM remote_flight.roadmap_rm16.t WHERE i = 1;
----
10
